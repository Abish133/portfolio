  "description": "A stunning, enterprise-grade portfolio website featuring cutting-edge 3D animations, smooth GSAP transitions, and modern web technologies. Built to MNC (Multinational Corporation) standards with exceptional performance and user experience.",
  "main": "index.html",
  "scripts": {
    "start": "npx serve . -p 8000",
    "dev": "npx live-server --port=8000 --open=/index.html",
    "build": "npm run minify && npm run optimize",
    "minify": "npx clean-css-cli -o dist/styles.min.css styles.css && npx terser -o dist/script.min.js script.js",
    "optimize": "npx imagemin images/* --out-dir=dist/images",
    "lighthouse": "npx lighthouse http://localhost:8000 --output=html --output-path=./lighthouse-report.html",
    "test": "npx playwright test",
    "deploy": "npm run build && npx vercel --prod",
    "lint": "npx eslint script.js",
    "format": "npx prettier --write ."
  },
